# SDS Node Configuration Template
# Copy this file to .env and fill in your actual values

# Your public IP address or hostname
NETWORK_ADDRESS=your.public.ip.address

# Your mnemonic phrase (24 words)
MNEMONIC_PHRASE="your mnemonic phrase here"

# Network port (default: 18081)
NETWORK_PORT=18081

# SDS version/branch to build (default: main)
SDS_VERSION=main

# User ID for running the container (default: 2048)
UID=2048

# Group ID for running the container (default: 2048)
GID=2048

